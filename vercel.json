

{
    
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "https://nodejs-hw-mongodb-auth-syz8.onrender.com"
        },
        {
          "key": "Access-Control-Allow-Credentials",
          "value": "true"
        },
        {
          "key": "Referrer-Policy",
          "value": "no-referrer-when-downgrade"
        }
      ]
    }
  ]
}
