

{
    
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
           "value": "default-src 'self'; img-src 'self' data: blob: https://66b1f8e71ca8ad33d4f5f63e.mockapi.io;"
        }
      ]
    }
  ]
}


